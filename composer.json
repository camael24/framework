{
    "name": "sohoa/framework",
    "require": {
        "php": ">=5.3.0",
        "hoa/core": "dev-master",
        "hoa/console": "dev-master",
        "hoa/http": "dev-master",
        "hoa/router": "dev-master",
        "hoa/view": "dev-master",
        "hoa/dispatcher": "dev-master",
        "hoa/session": "dev-master",
        "hoa/file": "dev-master",
        "hoa/registry": "dev-master",
        "hoa/stringbuffer": "dev-master"

    },
    "suggest": {
        "hoa/xyl": "To use Sohoa\\Framework\\View\\Xyl adapter in your project"
    },
    "require-dev": {
        "atoum/atoum": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Sohoa\\Framework": "Sources/",
            "Sohoa\\Bin": "Sources/"
        }
    },

    "bin": [
        "Sources/Sohoa/Bin/sohoa"
    ],
    "minimum-stability": "dev"
}
